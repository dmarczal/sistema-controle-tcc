<table class="table table-hovered">
    <tr>
        <th>TCC</th>
        <th>Anexo</th>
        <th>Aprovar</th>
        <th>Reprovar</th>
    </tr>
    <% @items.each do |item| %>
    <tr>
        <td><%= item.timeline.to_s %></td>
        <td><%= link_to item.file, item.file %></td>
        <td><%= link_to 'Aprovar', teachers_approve_path(item.timeline, item), class: 'btn btn-xs btn-success' %></td>
        <td><%= link_to 'Reprovar', teachers_repprove_path(item.timeline, item), class: 'btn btn-xs btn-danger' %></td>
    </tr>
    <% end %>
</table>